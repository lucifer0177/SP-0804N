{
  "name": "frontend",
  "version": "1.0.0",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "chart.js": "^4.3.0",
    "react-chartjs-2": "^5.2.0",
    "@heroicons/react": "^1.0.6",
    "tailwindcss": "^3.3.2",
    "postcss": "^8.4.23",
    "autoprefixer": "^10.4.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && tailwindcss build src/index.css -o src/styles.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "tailwindcss build src/index.css -o src/styles.css",
    "watch:css": "tailwindcss build src/index.css -o src/styles.css --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}